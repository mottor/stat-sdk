{
  "name": "mottor/stat-sdk",
  "license": "proprietary",
  "authors": [
    {
      "name": "Yuriy Pavlov",
      "email": "yuriypavlov@outlook.com"
    }
  ],
  "config": {
    "platform": {
      "php": "5.6"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Mottor\\Stat\\Domain\\": "src/Stat/Domain"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mottor\\Stat\\Test\\": "src/Stat/Test"
    }
  },
  "require": {
    "ext-json": "*",
    "mottor/api-sdk": "0.5.5",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mottor/api-sdk"
    }
  ]
}
